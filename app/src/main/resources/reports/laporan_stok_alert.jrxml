	<query language="sql"><![CDATA[SELECT CONCAT('MNU-', LPAD(id_masakan, 3, '0')) as kode, nama_masakan, stok, CASE   WHEN stok <= 0 THEN 'HABIS'   WHEN stok < 20 THEN 'MENIPIS'   ELSE 'AMAN' END as status_stok FROM menu_masakan WHERE stok < 20 ORDER BY stok ASC, nama_masakan]]></query>
