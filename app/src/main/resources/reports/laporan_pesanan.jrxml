	<query language="sql"><![CDATA[SELECT CONCAT('ORD-', LPAD(o.id_order, 3, '0')) as kode_order, o.tgl_pesan, o.tgl_kirim, p.nama as nama_pelanggan, o.jenis_pesanan, o.total, o.catatan FROM `order` o LEFT JOIN pelanggan p ON o.id_pelanggan = p.id_pelanggan ORDER BY o.tgl_pesan DESC]]></query>
