	<query language="sql"><![CDATA[SELECT DATE(tanggal) as tanggal, COUNT(id_transaksi) as jumlah_transaksi, SUM(total_bayar) as total_pendapatan FROM transaksi GROUP BY DATE(tanggal) ORDER BY tanggal DESC]]></query>
